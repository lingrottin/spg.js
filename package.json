{
  "name": "sspg",
  "version": "1.0.0",
  "description": "The Simplest Strong Password Generator",
  "type": "module",
  "main": "dist/sspg.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lingrottin/sspg.js.git"
  },
  "bugs": {
    "url": "https://github.com/lingrottin/sspg.js/issues"
  },
  "scripts": {
    "test": "jest",
    "lint": "biome check --fix",
    "build": "rimraf dist && tsc && rolldown -c rolldown.config.mts",
    "format": "biome format src --write",
    "watch": "tsc --watch"
  },
  "keywords": [
    "random",
    "generator",
    "string",
    "crypto",
    "utility",
    "typescript",
    "browser",
    "library",
    "password"
  ],
  "author": "University of Fool <uof@uof.edu.kg> (https://uof.edu.kg)",
  "license": "MIT",
  "packageManager": "pnpm@10.4.1",
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@biomejs/biome": "1.9.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "rolldown": "1.0.0-beta.3",
    "typescript": "^5.7.3"
  },
  "files": ["dist/**/*"]
}
